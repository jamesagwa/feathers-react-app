{
  "host": "recipes-app-app.feathersjs.com",
  "port": "PORT",
  "public": "../../client/build/",
  "staticUploads": "../uploads",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "authentication": {
    "secret": "SECRET",
    "strategies": [
      "jwt",
      "local"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  },
  "mongodb": "MONGO_URL"
}
